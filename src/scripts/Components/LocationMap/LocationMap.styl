@require '../../../styles/styleHelpers.styl';

.location-map {
  width: 400px;
  height: 400px;
  background: $light-gray;
  position: relative;
  .dot {
    background: red;
    width: 20px;
    height: 20px;
    border-radius: 10px;
    position: absolute;
    &.marker {
      background: blue;
    }
  }
}

